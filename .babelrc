{
  "presets": [
    ["env", {
      "modules": false
    }],
    "stage-2",
    "es2015"
  ],
  "plugins": ["transform-runtime",
    ["component", [
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ]],
    ["transform-class-properties", { "spec": true }]
  ]
}
